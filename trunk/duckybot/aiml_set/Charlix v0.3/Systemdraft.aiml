	<category>
		<pattern>QUESTION I</pattern>
		<template>
			Who is the best robot? A. R2D2. B. ALICE. C. HAL 9000.

			<think>
				<set name="topic">
					QUESTION I 
				</set>
			</think>
		</template>
	</category>
	
	
	<topic name="QUESTION I"> 
		
		<category> 
			<pattern>_</pattern>
			<template>
				<think>
					<set name="star"><star /></set>
				</think>
				<think><set name="topic">QUESTIONS</set></think>
				<condition>
					<li name="star" value="A">
						It's okay to guess.  Think harder about what the question is asking you.
						<srai>QUESTION I</srai>
					</li> 
					<li name="star" value="B">
						You are correct!  The robot ALICE is the "world's most human computer."  
						Now let us move on to the next question!
						<srai>QUESTION II</srai>
					</li>
					<li name="star" value="C">
						Not quite.  Remember, HAL 9000 is a fictional robot, not a real robot.  Try again!
						<srai>QUESTION I</srai> </li>
					<li>
						Please answer the quiz questions with just A, B or C. Try again!
						<srai>QUESTION I</srai>
					</li> 
				</condition>
			</template> </category> 
		
</topic> 


